#compdef warp

WARP=${WARP:-$HOME/.warp}

for w in $(cat "$WARP" 2>/dev/null | grep -v " $PWD$" | cut -d' ' -f1) ; do
  compadd -- "$w"
done

local w=$(cat "$WARP" 2>/dev/null | grep " $PWD$" | cut -d' ' -f1)
if [ -n "$w" ] ; then
  compadd -- "-$w"
  _message -- "-$w"
else
  _message -- "+${PWD:t:l}"
fi

# EOF
